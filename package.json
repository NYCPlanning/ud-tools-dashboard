    "ol": "^6.2.1",
    "proj4": "^2.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
